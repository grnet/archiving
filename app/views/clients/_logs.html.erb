<div class='row'>
  <div class='col-xs-3'>
    <h3>Logs</h3>
  </div>

  <div class="col-xs-12">
    <div class="table-responsive">
      <table class="table table-striped table-bordered table-condensed">
        <thead>
          <tr>
            <th>LogId</th>
            <th>Job</th>
            <th>Time</th>
            <th>Text</th>
          </tr>
        </thead>
        <tbody>
          <%= render partial: 'log', collection: @logs %>
        </tbody>
      </table>
    </div>
  </div>
</div>
